{
  "version": 2,
  "builds": [
    { "src": "api/send-visit-notification.js", "use": "@vercel/node" },
    { "src": "index.html", "use": "@vercel/static" },
    { "src": "assest/**", "use": "@vercel/static" }
  ],
  "routes": [
    { "src": "/api/(.*)", "dest": "/api/send-visit-notification" },
    { "src": "/(.*)", "dest": "/index.html" }
  ]
}
